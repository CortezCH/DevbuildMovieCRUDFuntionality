@model Movie

<div class="catalog-container">
    <h5>@Model.Title</h5>
    <h7>Genre:</h7>
    <p>@Model.Genre</p>

    <h7>Year:</h7>
    <p>@Model.Year</p>

    <h7>Run time:</h7>
    <p>@Model.RunTime</p>

    <p>
        <a asp-controller="Movie" asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
        <a asp-controller="Movie" asp-action="Delete" asp-route-id="@Model.ID">Delete</a>
    </p>
</div>